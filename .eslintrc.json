{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "globals": {
    "$": false,
    "Modernizr": false
  },
  "rules": {
    "comma-dangle": 0,
    "space-before-function-paren": 0,
    "max-len": 0,
    "func-names": 0,
    "import/no-unresolved": 0,
    "arrow-body-style": [2, "as-needed"],
    "consistent-return": 0
  }
}
